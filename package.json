{
  "name": "gamer",
  "version": "1.0.0",
  "description": "This project is an implementation of a game application which runs in the browser, using Phaser3 technology.",
  "main": "src/index.js",
  "scripts": {
    "watch": "webpack --mode=development --watch",
    "build": "webpack -p --env production",
    "dev": "webpack-dev-server --mode=development --env development --watch --open",
    "debug": "webpack-dev-server  --open",
    "start": "webpack-dev-server --env production --open",
    "heroku-start": "yarn build && NODE_ENV=production node server.js",
    "dev-start": "npm run build && webpack-dev-server --port=8000",
    "eslint-f": "npx eslint . --fix",
    "stylelint-f": "npx stylelint **/*.{css,scss} --fix"
  },
  "author": "Bluette1 <marylene.sawyer@gmail.com>",
