  "bugs": {
    "url": "https://github.com/bluette1/gamer/issues"
  },
  "homepage": "https://github.com/bluette1/gamer#readme",
  "dependencies": {
    "babel-loader": "^8.2.1",
    "normalize.css": "^8.0.1",
    "phaser": "^3.3.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^2.0.0",
    "bootstrap": "^4.5.2",
    "css-loader": "^4.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.1.1",
